function sc(array){
  var soretdArr = array.sort(function(a,b){
    return a-b;
    });
  var odArr=[];
  var evenArr=[];

    for (var i = 0; i < soretdArr.length; i++){
    if (i % 2 === 0){
    odArr.push(soretdArr[i]);
  } else {
     evenArr.push(soretdArr[i]);
    }

}
return odArr.concat(evenArr);
}
